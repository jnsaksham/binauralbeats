    <Routine name="Code">
      <CodeComponent name="code_2">
        <Param val="import os&amp;#10;import random&amp;#10;import numpy&amp;#10;&amp;#10;def gen_seq(files, master_path, shifted_path):&amp;#10;    paths = list('N')&amp;#10;    for fp in files:&amp;#10;        master = master_path + fp&amp;#10;        paths.append(master)&amp;#10;        shifted = shifted_path + fp.split('.')[0] + '_shifted.wav'&amp;#10;        paths.append(shifted)&amp;#10;    # Randomize the array&amp;#10;    random.shuffle(paths)&amp;#10;    # Sort array to avoid repetition&amp;#10;    paths = sequence_final(sequence)&amp;#10;    return paths&amp;#10;&amp;#10;def trim_txt(string):&amp;#10;    tmp = string.split('/')[-1].split('.')[0].split('_')[0]&amp;#10;    return tmp&amp;#10;&amp;#10;def sequence_final(sequence):&amp;#10;    string = sequence[0]&amp;#10;    new_sequence = np.array(string)&amp;#10;    tmps = np.array(trim_txt(string))&amp;#10;    i = 1&amp;#10;    while i &lt;= len(sequence)-1:&amp;#10;        tmp = trim_txt(sequence[i])&amp;#10;        tmps = np.append(tmps, tmp)&amp;#10;        if tmp == tmps[i-1]:&amp;#10;            new_sequence = np.append(new_sequence, sequence[i+1])&amp;#10;            new_sequence = np.append(new_sequence, sequence[i])&amp;#10;            i = i + 2&amp;#10;        else:&amp;#10;            new_sequence = np.append(new_sequence, sequence[i])&amp;#10;            i += 1&amp;#10;    return new_sequence&amp;#10;    &amp;#10;master_path = 'audio/master/'&amp;#10;shifted_path = 'audio/shifted/'&amp;#10;all_files = os.listdir(master_path)&amp;#10;&amp;#10;# File index for tracking condition&amp;#10;index = 0&amp;#10;&amp;#10;# Select 2 files&amp;#10;files = random.sample(all_files, 2)&amp;#10;&amp;#10;# Generate random sequence and relative path array&amp;#10;sequence = gen_seq(files, master_path, shifted_path)&amp;#10;np.savetxt('sequence.txt', np.array(sequence), fmt='%s')" valType="extendedCode" updates="constant" name="Before Experiment"/>
